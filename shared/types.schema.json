{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "Movie": {
            "additionalProperties": false,
            "properties": {
                "genre_ids": {
                    "items": {
                        "type": "number"
                    },
                    "type": "array"
                },
                "movieId": {
                    "type": "number"
                },
                "original_language": {
                    "type": "string"
                },
                "overview": {
                    "type": "string"
                },
                "popularity": {
                    "type": "number"
                },
                "release_date": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "video": {
                    "type": "boolean"
                },
                "vote_average": {
                    "type": "number"
                },
                "vote_count": {
                    "type": "number"
                }
            },
            "required": [
                "genre_ids",
                "movieId",
                "original_language",
                "overview",
                "popularity",
                "release_date",
                "title",
                "video",
                "vote_average",
                "vote_count"
            ],
            "type": "object"
        },
        "MovieReview": {
            "additionalProperties": false,
            "properties": {
                "content": {
                    "type": "string"
                },
                "movieId": {
                    "type": "number"
                },
                "reviewDate": {
                    "type": "string"
                },
                "reviewerName": {
                    "type": "string"
                }
            },
            "required": [
                "content",
                "movieId",
                "reviewDate",
                "reviewerName"
            ],
            "type": "object"
        },
        "MovieReviewQueryParams": {
            "additionalProperties": false,
            "properties": {
                "movieId": {
                    "type": "string"
                },
                "reviewDate": {
                    "type": "string"
                },
                "reviewerName": {
                    "type": "string"
                }
            },
            "required": [
                "movieId"
            ],
            "type": "object"
        }
    }
}

